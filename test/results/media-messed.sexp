((At media ((Identifier screen) (Any ", projection"))
  (Compound
   (((selectors ( "html\
                 \n  \
                 \n  "))
     (ruleset ((background ((Any #fffef0))) (color ((Any #300))))))
    ((selectors ( "body\
                 \n\
                 \n"))
     (ruleset ((max-width ((Any 35em))) (margin ((Any "0 auto")))))))))
 (At media ((Identifier print))
  (Compound
   (((selectors ("html "))
     (ruleset ((background ((Any #fff))) (color ((Any #000))))))
    ((selectors ("body "))
     (ruleset ((padding ((Any 1in))) (border ((Any "0.5pt solid #666"))))))))))